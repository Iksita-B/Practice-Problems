/*
Finding the smallest digit in the given number
Input: 618
Output: 1
*/
#include<stdio.h>
int main()
{
    int num,Number;
    scanf("%d",&num);
    int last,last2;
    last=num%10;
    num=num/10;
    while(num!=0)
    {
        last2=num%10;
        if(last2<=last)
        {
            last=last2;
        }
        num=num/10;
    }
    printf("%d",last);
    
    
}
